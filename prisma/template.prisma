
generator client {
  provider = "prisma-client-js"
  output   = "./generated/ACCOUNTID"
}

datasource db {
  provider = "postgresql"
  url      = env("SCHEMA_URL?schema=ACCOUNTID")
}

model Table {
  id        Int           @id @default(autoincrement())
  name      String?
  order     Order[]
}

model Order {
  id          Int         @id @default(autoincrement())
  table       Int
  tableId     Table       @relation(fields: [table], references: [id])
  product     Int
  productId   Product     @relation(fields: [product], references: [id])
  quantity    Int       
  price       Int       
  provided    Boolean
}

model Product{
  id          Int         @id @default(autoincrement())
  name        String      @unique
  price       Int
  order     Order[]
}