model Order {
  id          Int         @id @default(autoincrement())
  table       Int
  stock       Int
  stockId     Stock       @relation(fields: [stock], references: [id])
  quantity    Int       
  price       Int       
  provided    Boolean
}

model Stock {
  id          Int         @id @default(autoincrement())
  name        String      @unique
  price       Int
  order       Order[]
  log         Log[]
}

model Log {
  id          Int         @id @default(autoincrement())
  table       Int
  stock       Int
  stockId     Stock       @relation(fields: [stock], references: [id])
  quantity    Int       
  price       Int       
  timestamp   DateTime    @default(now())
}
