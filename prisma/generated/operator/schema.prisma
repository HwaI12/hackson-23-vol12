// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "./generated/operator"
}

datasource db {
  provider = "postgresql"
  url      = env("PUBLIC_URL")
}


model Account {
  id        String   @id
  name      String
  email     String   @unique
  password  String
  webpages  Webpage?
}

model Webpage {
  id        String    @unique
  account   Account   @relation(fields: [id], references: [id])
  color     Int
  table     Int
  menu      Json
}
